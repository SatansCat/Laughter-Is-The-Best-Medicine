.button {
    justify-content: space-between;
    align-self: center;
    align-items: center;
    -unity-background-scale-mode: scale-to-fit;
    background-color: rgba(94, 94, 94, 0);
    width: 100%;
    height: 20%;
    max-width: 100%;
    max-height: 20%;
    min-width: 75%;
    min-height: 15%;
    color: rgba(0, 0, 0, 0);
    -unity-background-image-tint-color: rgb(255, 255, 255);
}
